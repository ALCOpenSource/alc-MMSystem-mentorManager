<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/discussion_forum_nav_graph"
    app:startDestination="@id/editPostFragment">

    <fragment
        android:id="@+id/discussionForumFragment2"
        android:name="com.peculiaruc.alc_mmsystem_mentormanager.ui.disscussion_forum.DiscussionForumFragment"
        android:label="fragment_discussion_forum"
        tools:layout="@layout/fragment_discussion_forum" >
        <action
            android:id="@+id/action_discussionForumFragment2_to_commentFragment"
            app:destination="@id/commentFragment" />
        <action
            android:id="@+id/action_discussionForumFragment2_to_newTopicFragment"
            app:destination="@id/newTopicFragment" />
        <action
            android:id="@+id/action_discussionForumFragment2_to_editPostFragment"
            app:destination="@id/editPostFragment" />
    </fragment>
    <fragment
        android:id="@+id/newTopicFragment"
        android:name="com.peculiaruc.alc_mmsystem_mentormanager.ui.disscussion_forum.NewTopicFragment"
        android:label="fragment_new_topic"
        tools:layout="@layout/fragment_new_topic" >
    </fragment>

    <fragment
        android:id="@+id/commentFragment"
        android:name="com.peculiaruc.alc_mmsystem_mentormanager.ui.disscussion_forum.CommentsFragment"
        android:label="fragment_new_topic"
        tools:layout="@layout/fragment_comments" >
    </fragment>
    <fragment
        android:id="@+id/editPostFragment"
        android:name="com.peculiaruc.alc_mmsystem_mentormanager.ui.disscussion_forum.EditPostFragment"
        android:label="EditPostFragment" />

</navigation>