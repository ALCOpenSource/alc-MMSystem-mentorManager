<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <!-- Report Downloaded Successfully! -->
    <TextView
        android:id="@+id/report_down"
        android:layout_width="258dp"
        android:layout_height="30dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="51dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="51dp"
        android:gravity="center_horizontal"
        android:text="TEAM 7 MMS TASKS"
        android:textAppearance="@style/report_down"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <!-- Frame 351 -->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/frame_351"
        android:layout_width="318dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="21dp"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="21dp"
        android:background="@drawable/frame_351"
        android:clipToOutline="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/report_down">

        <!-- Done -->
        <TextView
            android:id="@+id/done"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:gravity="top"
            android:text="PROFILE"
            android:textAppearance="@style/download_generate"
            android:textSize="25dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>
    <!-- Frame 351 -->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/Reports"
        android:layout_width="318dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="20dp"
        android:background="@drawable/frame_351"
        android:clipToOutline="true"
        app:layout_constraintEnd_toEndOf="@+id/frame_351"
        app:layout_constraintStart_toStartOf="@+id/frame_351"
        app:layout_constraintTop_toBottomOf="@+id/frame_351">

        <!-- Done -->
        <TextView
            android:id="@+id/TXT_REPORTS"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:gravity="top"
            android:text="REPORTS"
            android:textAppearance="@style/download_generate"
            android:textSize="25dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <!-- Frame 351 -->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/Messages"
        android:layout_width="318dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="20dp"
        android:background="@drawable/frame_351"
        android:clipToOutline="true"
        app:layout_constraintEnd_toEndOf="@+id/frame_351"
        app:layout_constraintStart_toStartOf="@+id/frame_351"
        app:layout_constraintTop_toBottomOf="@+id/Reports">

        <!-- Done -->
        <TextView
            android:id="@+id/txt_messages"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:gravity="top"
            android:text="MESSSAGES"
            android:textAppearance="@style/download_generate"
            android:textSize="25dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <!-- Frame 351 -->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/Chat_Broadcast"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/frame_351"
        android:clipToOutline="true"
        app:layout_constraintEnd_toEndOf="@+id/frame_351"
        app:layout_constraintStart_toStartOf="@+id/frame_351"
        app:layout_constraintTop_toBottomOf="@+id/Messages">

        <!-- Done -->
        <TextView
            android:id="@+id/txt_Chat_Broadcast"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:gravity="center_horizontal"
            android:text="Chat / Broadcast Messages"
            android:textAppearance="@style/download_generate"
            android:textSize="25dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>



    <!-- Frame 351 -->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/discussion_forum"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/frame_351"
        android:clipToOutline="true"
        app:layout_constraintEnd_toEndOf="@+id/frame_351"
        app:layout_constraintStart_toStartOf="@+id/frame_351"
        app:layout_constraintTop_toBottomOf="@+id/Chat_Broadcast">

        <!-- Done -->
        <TextView
            android:id="@+id/txt_discussion_forum"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:gravity="center_horizontal"
            android:text="Discussion Forum"
            android:textAppearance="@style/download_generate"
            android:textSize="25dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>



</androidx.constraintlayout.widget.ConstraintLayout>