<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"

    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <!-- MM- Report Alert Dialog -->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/mm_report_a"
        android:layout_width="0dp"
        android:layout_height="391dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:clipToOutline="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <!-- Group 509 -->

        <!-- Add Mentors -->
        <TextView
            android:id="@+id/add_mentors"
            android:layout_width="101dp"
            android:layout_height="30dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="69dp"
            android:fontFamily="@font/mukta"
            android:gravity="top"
            android:text="@string/add_mentor"
            android:textColor="@color/mms_black_2"
            android:textFontWeight="600"
            android:textSize="18sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <!-- Email -->
        <TextView
            android:id="@+id/email"
            android:layout_width="28dp"
            android:layout_height="20dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:fontFamily="@font/mukta"
            android:gravity="top"
            android:text="@string/email"
            android:textColor="@color/mms_black_6"
            android:textFontWeight="400"
            android:textSize="12sp"
            app:layout_constraintStart_toStartOf="@+id/frame_351"
            app:layout_constraintTop_toBottomOf="@+id/add_mentors" />

        <!-- Frame 133 -->

        <!-- Samkebede@gmail.com -->
        <EditText
            android:id="@+id/samkebede_g"
            android:layout_width="0dp"
            android:layout_height="48dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginStart="21dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="21dp"
            android:background="@null"
            android:fontFamily="@font/mukta"
            android:gravity="top"
            android:hint="@string/samkebede_gmail_com"
            android:paddingStart="16dp"
            android:paddingTop="10dp"
            android:textColor="@color/mms_black_6"
            android:textFontWeight="400"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/email" />


        <!-- Report Downloaded Successfully! -->

        <!-- Frame 351 -->


        <Button
            android:id="@+id/frame_351"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="21dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="21dp"
            android:background="@drawable/button_background_filled"
            android:text="Done"
            android:textAllCaps="false"
            android:textFontWeight="700"
            android:textSize="16sp"
            app:fontFamily="@font/mukta"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/samkebede_g" />

        <Button
            android:id="@+id/frame_352"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="38dp"
            android:background="@drawable/button_border"
            android:text="Cancel"
            android:textAllCaps="false"
            android:textColor="@color/mms_pry_2"
            android:textFontWeight="700"
            android:textSize="16sp"
            app:fontFamily="@font/mukta"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/frame_351"
            app:layout_constraintStart_toStartOf="@+id/frame_351"
            app:layout_constraintTop_toBottomOf="@+id/frame_351" />


    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>