<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <!-- MM- Report Alert Dialog -->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/mm_report_a"
        android:layout_width="0dp"
        android:layout_height="391dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:clipToOutline="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <!-- Group 509 -->

        <!-- Add Mentors -->
        <TextView
            android:id="@+id/add_mentors"
            android:layout_width="101dp"
            android:layout_height="30dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="129dp"
            android:layout_marginTop="69dp"
            android:layout_marginEnd="130dp"
            android:fontFamily="@font/mukta"
            android:gravity="top"
            android:text="Add Mentor"
            android:textColor="@color/mms_black_2"
            android:textFontWeight="600"
            android:textSize="18dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <!-- Email -->
        <TextView
            android:id="@+id/email"
            android:layout_width="28dp"
            android:layout_height="20dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="21dp"
            android:layout_marginTop="19dp"
            android:fontFamily="@font/mukta"
            android:gravity="top"
            android:text="Email"
            android:textColor="@color/mms_black_6"
            android:textFontWeight="400"
            android:textSize="12dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/add_mentors" />

        <!-- Frame 133 -->
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/frame_133"
            android:layout_width="0dp"
            android:layout_height="48dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="21dp"
            android:background="@drawable/frame_133"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/email"
            app:layout_constraintTop_toBottomOf="@+id/email">
            <!-- Samkebede@gmail.com -->
            <EditText
                android:id="@+id/samkebede_g"
                android:layout_width="0dp"
                android:layout_height="48dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginStart="16dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="8dp"
                android:background="@null"
                android:fontFamily="@font/mukta"
                android:gravity="top"
                android:hint="Samkebede@gmail.com"
                android:textColor="@color/mms_black_6"
                android:textFontWeight="400"
                android:textSize="16dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>


        <!-- Report Downloaded Successfully! -->

        <!-- Frame 351 -->


        <!-- Frame 352 -->
        <RelativeLayout
            android:id="@+id/frame_351"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="36dp"
            android:background="@drawable/frame_351"
            android:clipToOutline="true"
            app:layout_constraintEnd_toEndOf="@+id/frame_133"
            app:layout_constraintStart_toStartOf="@+id/frame_133"
            app:layout_constraintTop_toBottomOf="@+id/frame_133">

            <!-- Done -->
            <TextView
                android:id="@+id/SEND"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="198dp"
                android:layout_marginLeft="133dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="130dp"
                android:layout_marginBottom="10dp"
                android:fontFamily="@font/mukta"
                android:gravity="center"
                android:text="Send"
                android:textAppearance="@style/share_Report"
                android:textFontWeight="600"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/frame_352"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="20dp"
            android:background="@drawable/frame_352"
            android:clipToOutline="true"
            app:layout_constraintEnd_toEndOf="@+id/frame_351"
            app:layout_constraintStart_toStartOf="@+id/frame_351"
            app:layout_constraintTop_toBottomOf="@+id/frame_351">
            <!-- Cancel -->
            <TextView
                android:id="@+id/cancel"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="187dp"
                android:layout_marginLeft="133dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="130dp"
                android:layout_marginBottom="14dp"
                android:gravity="center"
                android:text="@string/cancel"
                android:textAppearance="@style/cancel" />


        </RelativeLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>




</androidx.constraintlayout.widget.ConstraintLayout>