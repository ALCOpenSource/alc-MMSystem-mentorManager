            android:layout_height="wrap_content"
            app:checkedChip="@+id/chip_about"
            app:selectionRequired="true"
            app:singleLine="true"
            app:singleSelection="true">

            <com.google.android.material.chip.Chip
                android:id="@+id/chipAbout"
                style="?attr/materialButtonOutlinedStyle"
                android:backgroundTint="@color/mms_pry_10"
                app:chipStrokeColor="@color/mms_pry_8"
                app:chipStrokeWidth="1dp"
                android:textColor="@color/mms_black_2"
                android:textSize="15sp"
                app:chipCornerRadius="5dp"
                android:fontFamily="@font/mukta_regular"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="About" />

            <com.google.android.material.chip.Chip
                android:id="@+id/chipPrograms"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                style="?attr/materialButtonOutlinedStyle"
                android:fontFamily="@font/mukta_regular"
                android:textSize="15sp"
                app:chipCornerRadius="5dp"
                android:textColor="@color/mms_black_2"
                app:chipStrokeWidth="1dp"
                app:chipStrokeColor="@color/mms_pry_8"
                android:backgroundTint="@color/mms_pry_10"
                android:text="Programs" />

            <com.google.android.material.chip.Chip
